<?php

abstract class BTS_SagePay_Address_Abstract extends BTS_Object {
    
    protected $_addressType = null;
    
    protected $_fields = array(
        "customer_name",
        "firstnames",
        "surname",
        "address1",
        "address2",
        "city",
        "post_code",
        "state",
        "country",
        "phone",
    );
    
    protected $_protectData = true;
    
    public function getType() {
        return $this->_addressType;
    }
    
}
